{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*": {
      "timeout": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:5173, https://sentient-app.netlify.app"
      }
    }
  ]
}